function newStoke(arg,arg1)
	local uistroke1=Instance.new("UIStroke",arg)
	uistroke1.ApplyStrokeMode=arg1
	uistroke1.Thickness=3
end
function newCorner(arg,arg1)
	local UICorner1=Instance.new("UICorner",arg)
	UICorner1.CornerRadius=UDim.new(arg1,0)
end
