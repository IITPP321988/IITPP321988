local AntiVoidPart=Instance.new("Part",workspace)
AntiVoidPart.Anchored = true
AntiVoidPart.Name = "AntiVoidPartlol"
AntiVoidPart.Position = Vector3.new(167.466, 340, -258.982)
AntiVoidPart.Size = Vector3.new(2048, 1, 2048)
AntiVoidPart.Transparency = 1
AntiVoidPart.CanCollide = false
-------------------------------------------------------------------------
local CanUseCrasher=true
local antivoidOnOff=false
local spamMode=false
local CanUseRain=true 
-------------------------------------------------------------------------
SettMainButt1.MouseButton1Down:Connect(function()
	Main1.Visible=true
	Main2.Visible=false
	Main3.Visible=false
end)
SettMainButt2.MouseButton1Down:Connect(function()
	Main1.Visible=false
	Main2.Visible=true
	Main3.Visible=false
end)
SettMainButt3.MouseButton1Down:Connect(function()
	Main1.Visible=false
	Main2.Visible=false
	Main3.Visible=true
end)
HideButton.MouseButton1Down:Connect(function()
	if frame1.Visible==true then
		frame1.Visible=false
	else
		frame1.Visible=true
	end
end)
-------------------------------------------------------------------------
local s = game.Players.LocalPlayer.leaderstats.Glove.Value
buttonSpam.MouseButton1Down:Connect(function()
	if spamMode == false then
		spamMode = true
		buttonSpam.BackgroundColor3=Color3.fromRGB(0,255,0)
	else
		spamMode = false
		buttonSpam.BackgroundColor3=Color3.fromRGB(255,0,0)
	end
end)
buttonRemoveBrick.MouseButton1Down:Connect(function()
	for i, v in ipairs(workspace:GetChildren()) do
		if v:IsA("Part") and v:FindFirstChild("Mesh") then
			v:Remove()
		end
	end
end)
buttonAntiVoid.MouseButton1Down:Connect(function()
	if antivoidOnOff == false then
		antivoidOnOff = true
		buttonAntiVoid.BackgroundColor3=Color3.fromRGB(0,255,0)
		AntiVoidPart.CanCollide=true
		AntiVoidPart.Transparency=0.75
	else
		antivoidOnOff = false
		buttonAntiVoid.BackgroundColor3=Color3.fromRGB(255,0,0)
		AntiVoidPart.CanCollide=false
		AntiVoidPart.Transparency=1
	end
end)
buttonRainBrick.MouseButton1Down:Connect(function()
	if CanUseRain == true then
		CanUseRain = false
		game.Players.LocalPlayer.Character.Head:remove()
		task.wait(4)
		for i, v in ipairs(workspace:GetChildren()) do
			if v.Name == "SlapGloveStand" then
				if v:FindFirstChild("Glove") then
					if v.Glove.NAME.Value == "Minecraft" then
						fireclickdetector(v:FindFirstChild("Glove").ClickDetector)
					end
				end
			end
		end
		wait(0.25)
		game.Players.LocalPlayer.Character:PivotTo(workspace.NormalArena.PortalTrigger.CFrame)
		wait(1)
		game.Players.LocalPlayer.Character:PivotTo(workspace.Legend.Screen.CFrame*CFrame.new(0,500,0))
		for i, v in ipairs(workspace:GetChildren()) do
			if v.Name == "SlapGloveStand" then
				if v:FindFirstChild("Glove") then
					if v.Glove.NAME.Value == "Table" then
						fireclickdetector(v:FindFirstChild("Glove").ClickDetector)
					end
				end
			end
		end
		wait(1)
		game.Players.LocalPlayer.Character:PivotTo(workspace.Legend.Screen.CFrame*CFrame.new(0,500,0))
		if game.Players.LocalPlayer.Character:FindFirstChild("Minecraft") then
			for i=1,500 do
				game.Players.LocalPlayer.Character["Minecraft"].Event:FireServer("lego")
			end
			task.wait(2.5)
			game:GetService("TeleportService"):Teleport(game.PlaceId,game.Players.LocalPlayer)
			game.Players.LocalPlayer:Kick("rain script (kicked for save bricks) rejoin...")
			game.Players.LocalPlayer:Remove()
		else
			CanUseRain = true
			game.StarterGui:SetCore("SendNotification", {Title = "ERROR",Text = "you dont have minecraft",Icon = content,Duration = 5})
		end
	end
end)
buttonCrash.MouseButton1Down:Connect(function()
	if CanUseCrasher == true then
		game.Players.LocalPlayer.Character.Head:remove()
		CanUseCrasher = false
		task.wait(4)
		for i, v in ipairs(workspace:GetChildren()) do
			if v.Name == "SlapGloveStand" then
				if v:FindFirstChild("Glove") then
					if v.Glove.NAME.Value == "Minecraft" then
						fireclickdetector(v:FindFirstChild("Glove").ClickDetector)
					end
				end
			end
		end
		wait(0.25)
		game.Players.LocalPlayer.Character:PivotTo(workspace.NormalArena.PortalTrigger.CFrame)
		wait(1)
		game.Players.LocalPlayer.Character:PivotTo(workspace.Legend.Screen.CFrame*CFrame.new(0,500,0))
		for i, v in ipairs(workspace:GetChildren()) do
			if v.Name == "SlapGloveStand" then
				if v:FindFirstChild("Glove") then
					if v.Glove.NAME.Value == "Table" then
						fireclickdetector(v:FindFirstChild("Glove").ClickDetector)
					end
				end
			end
		end
		wait(1)
		game.Players.LocalPlayer.Character:PivotTo(workspace.Legend.Screen.CFrame*CFrame.new(0,500,0))
		if game.Players.LocalPlayer.Character:FindFirstChild("Minecraft") then
			for i=1,500 do
				game.Players.LocalPlayer.Character["Minecraft"].Event:FireServer("lego")
			end
			task.wait(2.5)
			game:GetService("TeleportService"):Teleport(game.PlaceId,game.Players.LocalPlayer)
			game.Players.LocalPlayer:Kick("rain script (kicked for save bricks) rejoin...")
			game.Players.LocalPlayer:Remove()
		else
			CanUseCrasher = true
			game.StarterGui:SetCore("SendNotification", {Title = "ERROR",Text = "you dont have minecraft",Icon = content,Duration = 5})
		end
	end
end)
game:GetService("RunService").RenderStepped:Connect(function()
	if spamMode == true then
		for i,v in ipairs(game.Players.LocalPlayer.Character:GetDescendants()) do
			if v:IsA("RemoteEvent") and v:FindFirstAncestorOfClass("Tool") then
				if s=="BOBBLE" or s=="Minecraft" or s=="Nerfer" or s=="PongPing" or s=="Subspace Tripmine" or s=="Purple" or s=="Builderman" then
					v:FireServer("lego")
				elseif s=="Table" or s=="Fuga" then
					v:FireServer("Move")
				elseif s=="Imaginary" then
					v:FireServer(20)
				elseif s=="Train" then
					v:FireServer("TRAIN")
				end
			end
		end
	end
end)
